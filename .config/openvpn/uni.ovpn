# split tunneling: use tunnel only for traffic to university network
#
#pull-filter ignore redirect-gateway
#pull-filter ignore "route-ipv6 "
#route 141.99.10.128 255.255.255.192 net_gateway
#route 141.99.0.0 255.255.0.0
#route-ipv6 2a04:dfc0::/32

# Ubuntu / Debian: update DNS servers automatically
# requires 'resolvconf' package
#
#script-security 2
#up /etc/openvpn/update-resolv-conf
#down /etc/openvpn/update-resolv-conf
